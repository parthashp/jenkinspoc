                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('Prod_Deploy_new') {
            steps {
                echo 'Deploying New Production environment....'
            }
        }
    }
}
